{
    "name": "ncclient",
    "version": "1.0.0",
    "description": "NetComposer Javascript client",
    "main": "server/server.js",
    "directories": {
        "doc": "docs"
    },
    "eslintConfig": {
        "plugins": [
            "html"
        ],
        "settings": {
            "html/xml-mode": true
        },
        "env": {
            "browser": true,
            "es6": true
        },
        "globals": {
            "angular": true,
            "Modernizr": true,
            "google": true,
            "_": true
        },
        "parser": "babel-eslint",
        "parserOptions": {
            "sourceType": "module",
            "ecmaVersion": 6,
            "allowImportExportEverywhere": false
        },
        "rules": {
            "camelcase": 0,
            "comma-spacing": 0,
            "curly": 0,
            "eol-last": 0,
            "key-spacing": 0,
            "no-multi-spaces": 0,
            "no-trailing-spaces": 0,
            "no-spaced-func": 0,
            "no-empty": 0,
            "no-shadow": 1,
            "no-unused-vars": 0,
            "no-use-before-define": 1,
            "no-underscore-dangle": 0,
            "quotes": 0,
            "semi": 0,
            "semi-spacing": 0,
            "strict": 0,
            "space-infix-ops": 0
        }
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/NetComposer/NcClient.git"
    },
    "author": "",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/NetComposer/NcClient/issues"
    },
    "homepage": "https://github.com/NetComposer/NcClient#readme",
    "devDependencies": {
        "babel-core": "^6.14.0",
        "babel-eslint": "^6.1.2",
        "babel-loader": "^6.2.5",
        "babel-plugin-transform-class-properties": "^6.11.5",
        "babel-polyfill": "^6.13.0",
        "babel-preset-es2015": "^6.14.0",
        "browser-sync": "latest",
        "eslint": "^3.5.0",
        "eslint-plugin-html": "^1.5.3",
        "gulp": "latest",
        "gulp-clean": "^0.3.2",
        "gulp-concat": "^2.6.0",
        "gulp-eslint": "^3.0.1",
        "gulp-if": "^2.0.1",
        "gulp-load-plugins": "latest",
        "gulp-mocha": "^3.0.1",
        "gulp-nodemon": "^2.1.0",
        "gulp-sourcemaps": "^1.6.0",
        "gulp-uglify": "^2.0.0",
        "gulp-util": "^3.0.7",
        "nodemon": "^1.10.2",
        "vinyl-named": "^1.1.0",
        "webpack": "^1.13.2",
        "webpack-dev-server": "^1.16.1",
        "webpack-stream": "^3.2.0"
    },
    "dependencies": {
        "express": "^4.14.0",
        "http-proxy": "^1.15.1",
        "morgan": "^1.7.0"
    }
}
